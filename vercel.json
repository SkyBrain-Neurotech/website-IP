{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "server/api/contact.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/api/contact.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}